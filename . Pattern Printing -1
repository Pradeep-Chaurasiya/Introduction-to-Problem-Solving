public class Solution {
    public int[][] solve(int A) {
        
        int B[][] = new int[A][];
        
        for(int i=0; i<A; i++)
        {
            B[i] = new int[i+1];
            for(int j=0; j<=i; j++)
            {
                B[i][j]=j+1;
            }
        }
        return B;
    }
}
